{
    "name" : "mymodule",
    "description" : "",
    "version" : "0.1.0",
    "author" : " <>",
    "contributors" : [],
    "repository" : {
        "type" : "git",
        "url" : "git://github.com//mymodule.git"
    },
    "bugs" : "http://github.com//mymodule/issues",
    "publishConfig" : {
        "registry" : "https://registry.npmjs.org"
    },
    "engines" : {
        "node" : "~0.10.22"
    },
    "dependencies": {
        "express": "^4.0.0",
        "swaggerize-express": "^3.0.0"
    },
    "devDependencies": {
        "tape": "^2.4.2",
        "istanbul": "^0.2.3",
        "jshint": "^2.4.1" ,
        "supertest": "^0.13.0"
    },
    "scripts": {
        "test": "tape test/*.js",
        "cover": "istanbul cover tape -- test/*.js",
        "lint": "jshint -c .jshintrc lib/**/*.js"
    },
    "scripts" : {
        "test" : "tape resources/tests/*.js",
        "cover" : "istanbul cover tape -- resources/tests/*.js",
        "lint" : "jshint -c .jshintrc index.js resources/handlers lib",
        "run": "node index.js",
        "postinstall": "swaggerize --api config/api.json --handlers handlers --tests tests"
    },
    "main" : "./lib/index",
    "readme" : "README.md"
}
